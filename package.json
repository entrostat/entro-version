{
  "name": "entro-version",
  "description": "Creates the correct version number and changelog using git-flow",
  "version": "1.2.9",
  "author": "Kerren @Kerren-Entrostat",
  "bin": {
    "entro-version": "./bin/run"
  },
  "bugs": "https://github.com/entrostat/entro-version/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "^2.4.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/entrostat/entro-version",
  "keywords": [
    "oclif",
    "cli",
    "version",
    "standard",
    "standard-version",
    "git",
    "flow",
    "git-flow",
    "release",
    "tag",
    "tagging"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "entro-version"
  },
  "repository": "entrostat/entro-version",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "release": "./bin/run --during-release-post-hook=\"npm publish && git commit -am 'Updated the readme'\""
  },
  "types": "lib/index.d.ts"
}
